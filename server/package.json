{
    "name": "xiaomi-unlock-server",
    "version": "1.0.0",
    "description": "Secure API server for Android device unlock operations",
    "main": "src/app.js",
    "scripts": {
        "start": "NODE_ENV=production node src/app.js",
        "start:dev": "NODE_ENV=development node src/app.js",
        "dev": "NODE_ENV=development nodemon src/app.js",
        "prod": "NODE_ENV=production pm2 start ecosystem.config.js --env production",
        "migrate": "node migrations/init.js",
        "migrate:prod": "NODE_ENV=production node migrations/init.js",
        "test": "NODE_ENV=test jest",
        "test:watch": "NODE_ENV=test jest --watch",
        "test:coverage": "NODE_ENV=test jest --coverage",
        "lint": "eslint src/ --fix",
        "security:audit": "npm audit",
        "security:check": "npm audit --audit-level moderate",
        "logs:view": "pm2 logs",
        "logs:flush": "pm2 flush",
        "status": "pm2 status",
        "stop": "pm2 stop ecosystem.config.js",
        "restart": "pm2 restart ecosystem.config.js",
        "reload": "pm2 reload ecosystem.config.js"
    },
    "keywords": [
        "android",
        "edl",
        "brom",
        "xiaomi",
        "unlock",
        "frp"
    ],
    "author": "Xiaomi Unlock System",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.6.0",
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-mongo-sanitize": "^2.2.0",
        "express-rate-limit": "^7.1.5",
        "express-slow-down": "^2.0.1",
        "express-validator": "^7.0.1",
        "helmet": "^7.1.0",
        "hpp": "^0.2.3",
        "joi": "^17.11.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "pg": "^8.11.3",
        "rate-limiter-flexible": "^2.4.2",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "nodemon": "^3.0.1",
        "supertest": "^6.3.3"
    }
}